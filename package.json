{
  "name": "webdsl",
  "displayName": "WebDSL",
  "description": "WebDSL",
  "version": "0.0.1",
  "publisher": "William Cotton",
  "author": {
    "name": "William Cotton",
    "email": "williamcotton@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/williamcotton/webdsl"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "webdsl",
      "aliases": ["WebDSL", "webdsl"],
      "extensions": [".webdsl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "webdsl",
      "scopeName": "source.webdsl",
      "path": "./syntaxes/webdsl.tmLanguage.json",
      "embeddedLanguages": {
        "meta.embedded.block.html": "html",
        "meta.embedded.block.css": "css",
        "meta.embedded.block.sql": "sql",
        "meta.embedded.block.jq": "jq",
        "meta.embedded.block.lua": "lua",
        "meta.embedded.block.mustache": "mustache"
      }
    }]
  }
}
